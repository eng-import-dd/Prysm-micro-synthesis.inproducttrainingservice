    <PackageReference Include="Synthesis.Nancy.Autofac.Module.Validation" Version="[1.0.*,1.1)" />
    <PackageReference Include="Synthesis.PolicyEvaluator.Autofac" Version="[1.0.*,1.1)" />
    <PackageReference Include="Synthesis.Tracking.ApplicationInsights" Version="[1.0.*,1.1)" />
    <PackageReference Include="Synthesis.InProductTrainingService.InternalApi" Version="[1.0.*,1.1)" />
    <PackageReference Include="Synthesis.PrincipalService.InternalApi" Version="[1.0.*,1.1)" />
